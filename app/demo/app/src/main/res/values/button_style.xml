<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="Button4">
        <item name="cornerSize">20%</item>

    </style>
    <style name="Button5">
        <item name="cornerSize">50%</item>

    </style>
    <style name="Button6">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
        <item name="android:background">@color/yellow</item>

    </style>

    <style name="Switch">
        <!-- active thumb & track color (30% transparency) -->
        <item name="colorControlActivated">#f1f1f1</item>

        <!-- inactive thumb color -->
        <item name="colorSwitchThumbNormal">#f1f1f1
        </item>

        <!-- inactive track color (30% transparency) -->
        <item name="android:colorForeground">#f1f1f1
        </item>
    </style>

    <style name="AlertDialogTheme" parent="ThemeOverlay.AppCompat.Dialog.Alert">
        <item name="buttonBarNegativeButtonStyle">@color/green</item>
        <item name="buttonBarPositiveButtonStyle">@color/green</item>
    </style>

    <style name="activityAnimation" parent="@android:style/Animation">
        <item name="android:activityOpenEnterAnimation">@anim/right_enter</item>
        <item name="android:activityOpenExitAnimation">@anim/left_exit</item>
        <item name="android:activityCloseEnterAnimation">@anim/left_enter</item>
        <item name="android:activityCloseExitAnimation">@anim/right_exit</item>
    </style>

</resources>